//将顺序表L的所有元素逆置,要求算法的空间复杂度为O(1)
#include <iostream>
using namespace std;
#define InitSize 50

typedef struct{
	int* data;
	int length, maxsize;
}SqList;

void Init(SqList& L)
{
	L.data = new int[InitSize];
	L.length = 10;
	for (int i = 0; i < L.length; i++)
	{
		cin >> L.data[i];
	}
}

bool Reverse(SqList& L)
{
	int i = 0, temp;
	for(i = 0; i < L.length/2; i++)
	{
		temp = L.data[i];
		L.data[i] = L.data[L.length-i-1];
		L.data[L.length-i-1] = temp;
	}
	return true;
}

int main() {
	SqList L;
	Init(L);
	Reverse(L);
	for (int i = 0; i < L.length; i++)
		cout << L.data[i] << " ";
	return 0;
}


